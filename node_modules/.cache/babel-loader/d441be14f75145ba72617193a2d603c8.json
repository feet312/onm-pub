h1 class=\"skSignet-logo\">\r\n        <img src=\"@/assets/images/skSignet-logo.png\" alt=\"skSignet\">\r\n      </h1>\r\n      <!-- SIDE MENU -->\r\n      <div class=\"menu\">\r\n        <ul>\r\n          <li\r\n            v-for=\"(menu, idx) in allMenuLists\"\r\n            :key=\"menu.idx\">\r\n            <dl>\r\n              <dt>\r\n                <a href=\"#\"\r\n                  @click=\"aa\">\r\n                  <span :class=\"['menuIcon-' + idx]\" />\r\n                    {{ menu.menuTitle }}\r\n                  <i class=\"icon icon-default icon-arrowDown-white\" />\r\n                </a>\r\n              </dt>\r\n              <dd>\r\n                <transition name=\"slide\">\r\n\r\n                    <template\r\n                      v-if=\"menu.children\">\r\n                      <div class=\"depth2\">\r\n                        <ul>\r\n                          <li\r\n                            v-for=\"menu in menu.children\"\r\n                            :key=\"menu.idx\">\r\n                            <a href=\"#\">{{ menu.menuTitle }}</a>\r\n                            <template\r\n                              v-if=\"menu.child\">  \r\n                              <div class=\"depth3\">\r\n                                <ul>\r\n                                  <li\r\n                                    v-for=\"menu in menu.child\"\r\n                                    :key=\"menu.idx\">\r\n                                    <a href=\"#\">{{ menu }}</a>\r\n                                  </li>\r\n                                </ul>\r\n                              </div>\r\n                            </template>\r\n                          </li>\r\n                        </ul>\r\n                      </div>\r\n                    </template>\r\n\r\n                </transition>\r\n              </dd>\r\n            </dl>\r\n          </li>\r\n        </ul>\r\n      </div>\r\n    </div>\r\n    <button\r\n      type=\"butto