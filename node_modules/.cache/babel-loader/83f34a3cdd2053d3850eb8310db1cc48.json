{"ast":null,"code":"import HeaderTop from '@/components/HeaderTop';\nimport SideBar from '@/components/SideBar';\nimport FooterBottom from '@/components/FooterBottom';\nimport ContentTitle from '@/components/ContentTitle';\nimport StatusData from '@/components/StatusData';\nimport SearchBtnOption from '@/components/SearchBtnOption';\nimport CheckBox from '@/components/CheckBox';\nimport DatePicker from '@/components/DatePicker';\nimport PageCount from '@/components/PageCount';\nimport SearchFilter from '@/components/SearchFilter';\nimport BtnResetSearch from '@/components/BtnResetSearch';\nimport BtnDownloadSearch from '@/components/moSearch/BtnDownloadSearch.vue';\nimport ModalMobileSearch from '@/components/moSearch/ModalMobileSearch';\nexport default {\n  components: {\n    HeaderTop,\n    SideBar,\n    FooterBottom,\n    ContentTitle,\n    StatusData,\n    SearchBtnOption,\n    CheckBox,\n    DatePicker,\n    PageCount,\n    SearchFilter,\n    BtnResetSearch,\n    BtnDownloadSearch,\n    ModalMobileSearch\n  },\n  props: {},\n  data() {\n    return {\n      title: \"충전기 상태\",\n      isActive: false,\n      searchIsActive: true,\n      checkboxShow: false,\n      searchOption: true,\n      location: ['대시보드', '충전인프라 관리', '충전기 상태 및 제어', '충전기 상태'],\n      statusLists: [{\n        name: '끊김',\n        number: 1\n      }, {\n        name: '대기',\n        number: 12\n      }, {\n        name: '준비',\n        number: 99\n      }, {\n        name: '예약',\n        number: 1\n      }, {\n        name: '충전',\n        number: 9\n      }, {\n        name: '종료',\n        number: 1\n      }, {\n        name: '고장',\n        number: 1\n      }],\n      checkBoxLists: [{\n        id: 1,\n        label: \"checkbox-1\",\n        checked: false\n      }, {\n        id: 2,\n        label: \"checkbox-2\",\n        checked: false\n      }, {\n        id: 3,\n        label: \"checkbox-3\",\n        checked: false\n      }],\n      searchFilters: [{\n        filterTitle: '권경',\n        filterText: '서울특별시'\n      }, {\n        filterTitle: '충전소ID',\n        filterText: 'SIG000003'\n      }, {\n        filterTitle: '충전기구분',\n        filterText: '완속'\n      }, {\n        filterTitle: '운영시작일',\n        filterText: '2022-10-22'\n      }],\n      items: ['아이템-1', '아이템-2', '아이템-3'],\n      windowSize: 0,\n      deviceSideisActive: false,\n      isActiveModal: false\n    };\n  },\n  created() {},\n  mounted() {\n    window.addEventListener('resize', () => {\n      this.resize();\n    });\n    this.resize();\n  },\n  watch: {},\n  methods: {\n    sideMenuToggle() {\n      this.isActive = !this.isActive;\n    },\n    searchToggle(e) {\n      if (e === 'downLoadOption') {\n        this.searchIsActive = true;\n        this.searchOption = false;\n        this.checkboxShow = true;\n      } else {\n        this.searchIsActive = !this.searchIsActive;\n        this.searchOption = true;\n        this.checkboxShow = false;\n      }\n    },\n    mobileSearchOpen(e) {\n      this.searchToggle(e);\n      this.isActiveModal = true;\n    },\n    modalClose() {\n      this.isActiveModal = false;\n    },\n    checkbox({\n      id,\n      checked\n    }) {\n      console.log(id, checked);\n      let index = this.checkBoxLists.findIndex(checkBoxList => {\n        return checkBoxList === id;\n      });\n      this.checkBoxLists[index].checked = checked;\n    },\n    // 디바이스 해상도 1200이하\n    resize() {\n      let targetName = document.getElementsByTagName('main');\n      let windowSize = window.outerWidth;\n      this.windowSize = windowSize;\n      if (this.windowSize <= 1200) {\n        if (targetName.classList.contains('sidebarClose')) {\n          targetName.classList.remove('sidebarClose');\n        } else {\n          targetName.classList.remove('deviceSideMenu-open');\n        }\n      }\n    },\n    deviceMenuToggle() {\n      this.deviceSideisActive = !this.deviceSideisActive;\n      this.isActive = false;\n      // this.$test(this.deviceSideisActive)\n    }\n  }\n};","map"